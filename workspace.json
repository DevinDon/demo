          "outputs": [
            "coverage/apps/weibo-api"
          ],
          "options": {
            "jestConfig": "apps/weibo-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "zhihu": {
      "projectType": "application",
      "root": "apps/zhihu",
      "sourceRoot": "apps/zhihu/src",
      "prefix": "demo",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
