{
  "version": 2,
  "projects": {
    "blog-2017": {
      "projectType": "application",
      "root": "apps/blog-2017",
      "sourceRoot": "apps/blog-2017/src",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/blog-2017",
            "index": "apps/blog-2017/src/index.html",
            "main": "apps/blog-2017/src/main.ts",
            "polyfills": "apps/blog-2017/src/polyfills.ts",
            "tsConfig": "apps/blog-2017/tsconfig.app.json",
            "assets": [
              "apps/blog-2017/src/favicon.ico",
              "apps/blog-2017/src/css",
              "apps/blog-2017/src/image",
              "apps/blog-2017/src/jquery",
              "apps/blog-2017/src/page"
            ],
            "styles": [
              "apps/blog-2017/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/blog-2017/src/environments/environment.ts",
                  "with": "apps/blog-2017/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "blog-2017:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "blog-2017:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/blog-2017/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/blog-2017"
          ],
          "options": {
            "jestConfig": "apps/blog-2017/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "blog-2017-e2e": {
      "root": "apps/blog-2017-e2e",
      "sourceRoot": "apps/blog-2017-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/blog-2017-e2e/cypress.json",
            "devServerTarget": "blog-2017:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "blog-2017:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/blog-2017-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "blog-2018": {
      "projectType": "application",
      "root": "apps/blog-2018",
      "sourceRoot": "apps/blog-2018/src",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/blog-2018",
            "index": "apps/blog-2018/src/index.html",
            "main": "apps/blog-2018/src/main.ts",
            "polyfills": "apps/blog-2018/src/polyfills.ts",
            "tsConfig": "apps/blog-2018/tsconfig.app.json",
            "assets": [
              "apps/blog-2018/src/favicon.ico",
              "apps/blog-2018/src/assets"
            ],
            "styles": [
              "apps/blog-2018/src/styles.less"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/blog-2018/src/environments/environment.ts",
                  "with": "apps/blog-2018/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "blog-2018:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "blog-2018:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/blog-2018/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/blog-2018"
          ],
          "options": {
            "jestConfig": "apps/blog-2018/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "blog-2018-e2e": {
      "root": "apps/blog-2018-e2e",
      "sourceRoot": "apps/blog-2018-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/blog-2018-e2e/cypress.json",
            "devServerTarget": "blog-2018:serve",
            "tsConfig": "apps/blog-2018-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "blog-2018:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/blog-2018-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "blog-over": {
      "projectType": "application",
      "root": "apps/blog-over",
      "sourceRoot": "apps/blog-over/src",
      "prefix": "demo",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/blog-over",
            "index": "apps/blog-over/src/index.html",
            "main": "apps/blog-over/src/main.ts",
            "polyfills": "apps/blog-over/src/polyfills.ts",
            "tsConfig": "apps/blog-over/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/blog-over/src/favicon.ico",
              "apps/blog-over/src/assets"
            ],
            "styles": [
              "apps/blog-over/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/blog-over/src/environments/environment.ts",
                  "with": "apps/blog-over/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "blog-over:build:production"
            },
            "development": {
              "browserTarget": "blog-over:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "blog-over:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/blog-over/src/**/*.ts",
              "apps/blog-over/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/blog-over"
          ],
          "options": {
            "jestConfig": "apps/blog-over/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "blog-over-e2e": {
      "root": "apps/blog-over-e2e",
      "sourceRoot": "apps/blog-over-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/blog-over-e2e/cypress.json",
            "devServerTarget": "blog-over:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "blog-over:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/blog-over-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/web"
  },
  "defaultProject": "blog-2018",
  "generators": {
    "@nrwl/web:application": {
      "style": "less",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/web:library": {
      "style": "less",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  }
}
