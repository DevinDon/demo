            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/zhihu",
            "index": "apps/zhihu/src/index.html",
            "main": "apps/zhihu/src/main.ts",
            "polyfills": "apps/zhihu/src/polyfills.ts",
            "tsConfig": "apps/zhihu/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
